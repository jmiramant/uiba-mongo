  "scripts": {
    "babel": "babel --presets es2015,react,stage-0 --ignore node_modules,public",
    "babel-node": "babel-node --presets es2015,react,stage-0 --ignore node_modules,public",
    "sequelize": "node_modules/.bin/sequelize",
    "lint": "eslint app server",
    "clean": "rimraf public && rimraf compiled",
    "start": "cross-env FROM_EMAIL=info@uiba.co NODE_ENV=production  node compiled/index.js",
    "dev": "cross-env FROM_EMAIL=info@uiba.co NODE_ENV=development nodemon",
    "build:dev": "cross-env FROM_EMAIL=info@uiba.co NODE_ENV=development webpack --colors --config ./webpack/webpack.config.dev-server.js",
    "build:prod": "cross-env FROM_EMAIL='info@uiba.co' NODE_ENV=production webpack --colors --config ./webpack/webpack.config.prod.js",
    "build:node": "cross-env FROM_EMAIL=info@uiba.co NODE_ENV=production npm run babel -- ./server -d ./compiled",
    "build": "npm run clean && npm run build:prod && npm run build:node",
    "test": "cross-env FROM_EMAIL=info@uiba.co NODE_ENV=test karma start",
    "test:watch": "cross-env FROM_EMAIL=info@uiba.co NODE_ENV=test npm test -- --watch --no-single-run",
    "postinstall": "npm run build"
  },
  "author": "Josh Miramant",
